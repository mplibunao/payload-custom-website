{
  "name": "@org/cms",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p \"build:*\"",
    "build:copyfiles": "copyfiles -u 1 \"cms/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "build:payload": "payload build",
    "build:server": "tsup",
    "dev": "run-p dev:*",
    "dev:node": "nodemon --exec 'ts-node --esm --swc -r tsconfig-paths/register ./src/server/index.ts' --watch ./src/server/index.ts",
    "dev:build": "cross-env APP=dev tsup --watch src/**/*",
    "start": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "format:fix": "prettier --ignore-path ../../.prettierignore --write 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
    "format:check": "prettier --ignore-path ../../.prettierignore --list-different 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
    "lint:check": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@org/shared": "workspace:*",
    "@sinclair/typebox": "^0.28.14",
    "close-with-grace": "^1.2.0",
    "env-schema": "^5.2.0",
    "express": "^4.18.2",
    "payload": "1.11.7"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/node": "^20.4.1",
    "esbuild": "^0.17.19",
    "eslint": "^8.44.0",
    "eslint-config-custom": "workspace:*",
    "glob": "10.3.3",
    "nodemon": "3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-config": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6",
    "tsup": "7.1.0"
  },
  "prettier": "prettier-config",
  "exports": {
    "./package.json": "./package.json",
    "./index": {
      "types": "./dist/esm/index.d.mts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs",
      "default": "./src/index.ts"
    },
    "./app": {
      "types": "./dist/esm/app.d.mts",
      "require": "./dist/cjs/app.js",
      "import": "./dist/esm/app.mjs",
      "default": "./src/app.ts"
    },
    "./infra/payload": {
      "types": "./dist/esm/infra/payload.d.mts",
      "require": "./dist/cjs/infra/payload.js",
      "import": "./dist/esm/infra/payload.mjs",
      "default": "./src/infra/payload.ts"
    },
    "./infra/config": {
      "types": "./dist/esm/infra/config.d.mts",
      "require": "./dist/cjs/infra/config.js",
      "import": "./dist/esm/infra/config.mjs",
      "default": "./src/infra/config.ts"
    },
    "./collections/FormSubmission": {
      "types": "./dist/esm/collections/FormSubmission.d.mts",
      "require": "./dist/cjs/collections/FormSubmission.js",
      "import": "./dist/esm/collections/FormSubmission.mjs",
      "default": "./src/collections/FormSubmission.ts"
    },
    "./container/registerLogger": {
      "types": "./dist/esm/container/registerLogger.d.mts",
      "require": "./dist/cjs/container/registerLogger.js",
      "import": "./dist/esm/container/registerLogger.mjs",
      "default": "./src/container/registerLogger.ts"
    },
    "./container/index": {
      "types": "./dist/esm/container/index.d.mts",
      "require": "./dist/cjs/container/index.js",
      "import": "./dist/esm/container/index.mjs",
      "default": "./src/container/index.ts"
    }
  }
}