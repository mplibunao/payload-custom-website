{
  "name": "@org/cms",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p \"build:*\"",
    "build:copyfiles": "copyfiles -u 1 \"cms/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "build:payload": "payload build",
    "build:server": "tsup",
    "dev": "run-p dev:*",
    "dev:node": "cross-env PAYLOAD_CONFIG_PATH=./src/infra/payload/config.ts nodemon --exec 'ts-node --swc -r tsconfig-paths/register ./src/index.ts' --watch ./src/index.ts",
    "dev:build": "cross-env APP=dev tsup --watch src/**/*",
    "start": "cross-env PAYLOAD_CONFIG_PATH=dist/cjs/infra/payload/config.js NODE_ENV=production node dist/index.js",
    "format:fix": "prettier --ignore-path ../../.prettierignore --write 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
    "format:check": "prettier --ignore-path ../../.prettierignore --list-different 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
    "lint:check": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@org/shared": "workspace:*",
    "@sinclair/typebox": "^0.28.14",
    "close-with-grace": "^1.2.0",
    "env-schema": "^5.2.0",
    "express": "^4.18.2",
    "overload-protection": "1.2.3",
    "payload": "1.11.7",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@swc/core": "1.3.71",
    "@swc/helpers": "0.5.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/node": "^20.4.1",
    "@types/overload-protection": "1.2.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.19",
    "eslint": "^8.44.0",
    "eslint-config-custom": "workspace:*",
    "glob": "10.3.3",
    "nodemon": "3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-config": "workspace:*",
    "ts-node": "10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "4.2.0",
    "tsup": "7.1.0",
    "typescript": "^5.1.6"
  },
  "prettier": "prettier-config",
  "exports": {
    "./package.json": "./package.json",
    "./lib/*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "default": "./src/*.ts"
    },
    "./lib/collections/FormSubmission": {
      "types": "./dist/esm/collections/FormSubmission.d.mts",
      "import": "./dist/esm/collections/FormSubmission.mjs",
      "default": "./src/collections/FormSubmission.ts"
    },
    "./lib/infra/payload/init": {
      "types": "./dist/esm/infra/payload/init.d.mts",
      "import": "./dist/esm/infra/payload/init.mjs",
      "default": "./src/infra/payload/init.ts"
    }
  }
}