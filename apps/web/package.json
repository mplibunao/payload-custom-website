{
	"name": "@payload-custom-website/web",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"clean": "rm -rf .turbo node_modules",
		"postinstall": "patch-package --patch-dir ./other/patches",
		"build": "run-s build:*",
		"build:remix": "remix build --sourcemap",
		"build:server": "tsx ./other/build-server.ts",
		"dev": "remix dev -c \"tsx watch --clear-screen=false --ignore 'app/**' --ignore 'build/**' --ignore 'node_modules/**' --inspect ./src/server/index.ts\" --no-restart",
		"format:fix": "prettier --ignore-path ../../.prettierignore --write 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
		"format:check": "prettier --ignore-path ../../.prettierignore --list-different 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
		"lint:check": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx --fix",
		"start": "cross-env NODE_ENV=production node .",
		"test": "vitest --coverage --bail=1",
		"test:watch": "vitest watch --coverage --ui",
		"coverage": "vitest run --coverage",
		"pretest:e2e:run": "npm run build",
		"typecheck": "tsc",
		"ci": "run-p \"test -- --run\" lint:check typecheck"
	},
	"dependencies": {
		"@remix-run/css-bundle": "^1.18.1",
		"@remix-run/express": "^1.18.1",
		"@remix-run/node": "^1.18.1",
		"@remix-run/react": "^1.18.1",
		"@remix-run/router": "^1.7.1",
		"@remix-run/server-runtime": "^1.18.1",
		"@sinclair/typebox": "^0.28.14",
		"chokidar": "^3.5.3",
		"class-variance-authority": "^0.6.1",
		"close-with-grace": "^1.2.0",
		"clsx": "^1.2.1",
		"compression": "^1.7.4",
		"confetti-react": "^2.5.0",
		"cross-env": "^7.0.3",
		"date-fns": "^2.30.0",
		"dotenv": "^16.3.1",
		"env-schema": "^5.2.0",
		"execa": "^7.1.1",
		"express": "^4.18.2",
		"get-port": "^7.0.0",
		"helmet": "^7.0.0",
		"intl-parse-accept-language": "^1.0.0",
		"isbot": "^3.6.12",
		"litefs-js": "^1.1.2",
		"lru-cache": "^10.0.0",
		"minimist": "^1.2.8",
		"prisma": "^5.0.0",
		"qrcode": "^1.5.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-transition-group": "^4.4.5",
		"remix-auth": "^3.5.0",
		"remix-auth-form": "^1.3.0",
		"remix-utils": "^6.6.0",
		"tailwind-merge": "^1.13.2",
		"tailwindcss-animate": "^1.0.6",
		"tailwindcss-radix": "^2.8.0",
		"thirty-two": "^1.0.2",
		"typebox-validators": "^0.2.22",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.0.2",
		"@ianvs/prettier-plugin-sort-imports": "3.7.2",
		"@playwright/test": "^1.35.1",
		"@remix-run/dev": "^1.18.1",
		"@remix-run/serve": "^1.18.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@total-typescript/ts-reset": "^0.4.2",
		"@types/bcryptjs": "^2.4.2",
		"@types/better-sqlite3": "^7.6.4",
		"@types/compression": "^1.7.2",
		"@types/cookie": "^0.5.1",
		"@types/express": "^4.17.17",
		"@types/express-serve-static-core": "^4.17.35",
		"@types/fs-extra": "^11.0.1",
		"@types/minimist": "^1.2.2",
		"@types/node": "^20.4.1",
		"@types/qrcode": "^1.5.1",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@types/react-transition-group": "^4.4.6",
		"@types/testing-library__jest-dom": "^5.14.7",
		"@vitejs/plugin-react": "^4.0.3",
		"@vitest/coverage-v8": "^0.33.0",
		"autoprefixer": "^10.4.14",
		"cookie": "^0.5.0",
		"enforce-unique": "^1.1.1",
		"esbuild": "^0.17.19",
		"esbuild-plugin-pino": "^2.0.0",
		"eslint": "^8.44.0",
		"eslint-config-custom": "workspace:*",
		"fs-extra": "^11.1.1",
		"jsdom": "^22.1.0",
		"msw": "^1.2.2",
		"npm-run-all": "^4.1.5",
		"patch-package": "^7.0.0",
		"pino": "^8.14.1",
		"prettier": "^2.8.7",
		"prettier-plugin-tailwindcss": "0.2.5",
		"prettier-plugin-sql": "^0.15.1",
		"remix-flat-routes": "^0.5.10",
		"tailwindcss": "^3.3.2",
		"tsconfig": "workspace:*",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"vite": "^4.4.2",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.33.0"
	}
}
