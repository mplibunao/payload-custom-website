{
	"name": "@payload-custom-website/web",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"clean": "rm -rf .turbo node_modules",
		"clean:turbo": "rm -rf .turbo",
		"build": "run-s build:*",
		"build:remix": "remix build --sourcemap",
		"build:payload": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts payload build",
		"build:server": "tsx ./scripts/build.ts --app 'prod'",
		"dev": "run-p dev:*",
		"dev:remix": "remix dev -c \"pnpm remix:dev\" --no-restart",
		"dev:types": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts nodemon --exec 'payload generate:types' --watch src/cms -e ts --ignore src/cms/payload-types.ts --ignore src/cms/generated-schema.graphql",
		"_remix:dev": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts nodemon --exec 'tsx ./scripts/devServer.js' --watch src/server --watch src/shared --watch src/cms -e ts",
		"remix:dev": "cross-env PAYLOAD_CONFIG_PATH=./src/payload.config.ts nodemon --exec 'ts-node --swc -r tsconfig-paths/register ./src/server/index.ts' --watch src/server --watch src/shared --watch src/cms -e ts",
		"format:fix": "prettier --ignore-path ../../.prettierignore --write 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
		"format:check": "prettier --ignore-path ../../.prettierignore --list-different 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
		"lint:check": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
		"lint:fix": "TIMING=1 eslint --color --cache --cache-location .eslintcache --ignore-path ../../.eslintignore ./src --ext .js,.jsx,.ts,.tsx --fix",
		"start": "cross-env NODE_ENV=production node .",
		"test": "vitest --coverage --bail=1",
		"test:watch": "vitest watch --coverage --ui",
		"typecheck": "tsc",
		"isEsm": "tsx ./scripts/checkEsm.ts",
		"routeJson": "remix routes --json",
		"routes": "run-p routes:* && pnpm format:fix",
		"routes:path": "remix-routes -o ./src/types",
		"routes:id": "tsx ./scripts/getRouteIds.ts",
		"icons": "node ./scripts/buildIcons.js && prettier --ignore-path ../../.prettierignore --write 'src/app/components/Icon/types.ts'",
		"compile": "tsx ./scripts/compileAjvSchemas.ts",
		"ui:add": "pnpm dlx shadcn-ui@latest add",
		"tw:gen": "tailwindcss-extend -p src/app/styles/tailwind.css"
	},
	"dependencies": {
		"@radix-ui/react-dialog": "1.0.4",
		"@radix-ui/react-toggle": "1.0.3",
		"@radix-ui/react-tooltip": "1.0.6",
		"@remix-run/css-bundle": "^1.18.1",
		"@remix-run/express": "^1.18.1",
		"@remix-run/node": "^1.18.1",
		"@remix-run/react": "^1.18.1",
		"@remix-run/router": "^1.7.1",
		"@remix-run/server-runtime": "^1.18.1",
		"@sinclair/typebox": "^0.28.14",
		"@tailwindcss/typography": "0.5.9",
		"close-with-grace": "^1.2.0",
		"cva": "npm:class-variance-authority@0.7.0",
		"dotenv": "16.3.1",
		"escape-html": "1.0.3",
		"express": "^4.18.2",
		"framer-motion": "10.16.1",
		"ioredis": "5.3.2",
		"isbot": "^3.6.12",
		"layered-loader": "11.0.1",
		"overload-protection": "1.2.3",
		"payload": "1.11.3",
		"pino": "8.14.1",
		"pino-http": "8.3.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"remix-custom-routes": "1.0.1",
		"remix-utils": "^6.6.0",
		"slate": "0.94.1",
		"slugify": "1.6.6",
		"tailwind-merge": "^1.13.2",
		"tailwindcss-animate": "1.0.6",
		"tslib": "2.6.1"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.18.1",
		"@remix-run/serve": "^1.18.1",
		"@swc/core": "1.3.71",
		"@swc/helpers": "0.5.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@total-typescript/ts-reset": "^0.4.2",
		"@types/compression": "^1.7.2",
		"@types/escape-html": "1.0.2",
		"@types/express": "^4.17.17",
		"@types/express-serve-static-core": "^4.17.35",
		"@types/minimist": "^1.2.2",
		"@types/node": "^20.4.1",
		"@types/overload-protection": "1.2.1",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@types/testing-library__jest-dom": "^5.14.7",
		"@vitejs/plugin-react": "^4.0.3",
		"@vitest/coverage-v8": "^0.33.0",
		"ajv": "8.12.0",
		"autoprefixer": "^10.4.14",
		"chalk": "5.3.0",
		"chokidar": "^3.5.3",
		"cross-env": "^7.0.3",
		"css-loader": "6.8.1",
		"esbuild": "^0.17.19",
		"eslint": "^8.44.0",
		"eslint-config-custom": "workspace:*",
		"execa": "7.2.0",
		"globby": "13.2.2",
		"icon-pipeline": "0.1.3",
		"jsdom": "^22.1.0",
		"minimist": "^1.2.8",
		"nodemon": "3.0.1",
		"npm-run-all": "^4.1.5",
		"pino-pretty": "10.2.0",
		"postcss": "8.4.27",
		"postcss-loader": "7.3.3",
		"prettier": "^2.8.7",
		"prettier-config": "workspace:*",
		"remix-routes": "1.4.4",
		"tailwindcss": "^3.3.2",
		"tailwindcss-extend": "0.0.3",
		"ts-node": "10.9.1",
		"tsconfig": "workspace:*",
		"tsconfig-paths": "4.2.0",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"vite": "^4.4.2",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.33.0"
	},
	"prettier": "prettier-config"
}
