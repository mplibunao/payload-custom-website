{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Remix CJS",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "esnext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"noEmit": true,
		"module": "NodeNext",
		"jsx": "react-jsx",
		"emitDeclarationOnly": false,
		"moduleResolution": "NodeNext",
		"incremental": false
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
